    "peerDependencyRules": {
      "ignoreMissing": [
        "@nestjs/common",
        "@nestjs/core"
      ]
    },
    "overrides": {
      "vite": "5.4.19"
    }
  },
  "scripts": {
    "build": "turbo build",
