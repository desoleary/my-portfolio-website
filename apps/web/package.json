{
  "name": "@my-portfolio/web",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "cross-env NEXT_DISABLE_ESLINT=1 NEXT_IGNORE_TYPE_CHECK=1 next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run  --passWithNoTests",
    "lint": "eslint . --ext .ts,.tsx,.js --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx,jsx,json,md,css,scss,yml,yaml}\"",
    "generate": "graphql-codegen --config codegen.ts",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "persist": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/build-persisted.ts",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.24.0",
    "@ant-design/icons": "^5.6.1",
    "@apollo/client": "^3.10.8",
    "antd": "^5.19.1",
    "dayjs": "^1.11.13",
    "graphql": "^16.9.0",
    "next": "^15.0.0",
    "next-seo": "^6.5.0",
    "next-sitemap": "^4.2.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "^5.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.1",
    "@my-portfolio/config": "workspace:^",
    "@testing-library/react": "^16",
    "@testing-library/user-event": "^14",
    "@types/node": "^20.19.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^2.0.5",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.2",
    "fast-glob": "^3.3.3",
    "jsdom": "^24.1.0",
    "prisma": "^5.18.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.19",
    "vitest": "^2.1.9"
  }
}
